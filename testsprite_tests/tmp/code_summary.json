  "features": [
    {
      "name": "Health Check",
      "description": "Endpoint to check api availability",
      "files": [
        "src/api/routes/health.py"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "paths": {
          "/health": {
            "get": {
              "summary": "Health Check",
              "operationId": "health_check_health_get",
              "responses": {
                "200": {
                  "description": "Successful Response",
                  "content": {
                    "application/json": {
                      "schema": {
                        "$ref": "#/components/schemas/HealthResponse"
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "components": {
          "schemas": {
            "HealthResponse": {
              "title": "HealthResponse",
              "type": "object",
              "properties": {
                "status": {
                  "title": "Status",
                  "type": "string"
                },
                "version": {
                  "title": "Version",
                  "type": "string"
                }
              },
              "required": [
                "status",
                "version"
              ]
            }
          }
        }
      }
    },
